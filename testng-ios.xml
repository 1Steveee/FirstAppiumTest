<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Appium Sample tests for IOS">
    <test name="WebDriverIO App IOS Tests" enabled="true">
        <classes>
            <class name="org.FirstAppiumTest.ios.AppiumTest">
                <methods>
                    <include name="testTitle"/>
                    <exclude name="testSignUp"/>
                    <exclude name="testLogin"/>
                    <include name="testFormsSignup"/>
                    <include name="testSwipe"/>
                    <include name="testSwipeAndFindElement"/>
                    <exclude name="testWebViewPage"/>
                    <include name="testDragAndDrop"/>
                </methods>
            </class>
        </classes>
    </test>
</suite> <!-- Suite -->